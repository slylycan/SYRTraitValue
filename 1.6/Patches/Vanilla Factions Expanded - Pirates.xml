<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Factions Expanded - Pirates</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>*/TraitDef[defName = "VFEP_WarcasketTrait"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
              <traitValues>
                <li>0</li>
              </traitValues>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>*/TraitDef[defName = "VFEP_Shellcasket"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
              <traitValues>
                <li>0</li>
              </traitValues>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
